{
    "name": "Getting Started Example",
    "description": "Build an Extension!",
    "version": "1.0",
    "manifest_version": 3,
    "content_scripts": [{
        "js": ["start.js"],
        "matches": ["https://orteil.dashnet.org/cookieclicker/*"]
      }],
    "web_accessible_resources": [{
        "resources": ["analyser.js"],
        "matches": ["<all_urls>"]
    }]
  }